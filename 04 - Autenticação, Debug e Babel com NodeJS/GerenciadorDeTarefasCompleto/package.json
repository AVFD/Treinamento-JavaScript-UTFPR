{
  "name": "gerenciador-de-tarefas-completo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-s build serve",
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "build": "babel . --delete-dir-on-start -d dist --copy-files --source-maps --ignore node_modules,dist,package.json,package-lock.json,yarn.lock",
    "dev": "cross-env NODE_ENV=development DEBUG=http:* nodemon index.js --exec babel-node"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.2",
    "cookie-parser": "1.4.3",
    "debug": "3.1.0",
    "express": "4.16.2",
    "jsonwebtoken": "8.1.0",
    "moment": "2.19.1",
    "mysql2": "1.4.2",
    "sequelize": "4.13.17"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "cross-env": "5.0.5",
    "nodemon": "1.12.1",
    "npm-run-all": "4.1.1",
    "sequelize-cli": "3.0.0",
    "sqlite3": "3.1.13"
  }
}
